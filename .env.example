# Database Configuration (Production)
DB_URL=jdbc:mysql://localhost:3306/trader_db
DB_DRIVER=com.mysql.cj.jdbc.Driver
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Logging Configuration (Production)
LOG_FILE_PATH=./logs

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Security Configuration
# Spring Boot Actuator 인증 (최소 16자 권장, 운영 환경 필수)
ACTUATOR_USERNAME=admin
ACTUATOR_PASSWORD=change_me_minimum_16_chars

# Redis 키 HMAC-SHA256 서명 시크릿 (최소 32자)
# 생성 방법: openssl rand -base64 32
REDIS_KEY_HMAC_SECRET=your_hmac_secret_minimum_32_chars_base64

# KIS 토큰 AES-256 암호화 키 (Base64 인코딩된 32바이트)
# 생성 방법: openssl rand -base64 32
TOKEN_ENCRYPTION_KEY=your_encryption_key_base64_32_bytes

# KIS (Korea Investment & Securities) API Configuration
# 한국투자증권 ID
KIS_ID=your_kis_id

# 퇴직연금 계좌
KIS_ACCOUNT_PENSION_NUMBER=12345678-01
KIS_ACCOUNT_PENSION_APP_KEY=your_app_key
KIS_ACCOUNT_PENSION_APP_SECRET=your_app_secret

# 개인투자 계좌
KIS_ACCOUNT_PERSONAL_NUMBER=87654321-01
KIS_ACCOUNT_PERSONAL_APP_KEY=your_app_key
KIS_ACCOUNT_PERSONAL_APP_SECRET=your_app_secret
